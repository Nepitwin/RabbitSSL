[
  {rabbit, [
     {ssl_listeners, [5671]},
     {ssl_options, [{cacertfile,"/etc/ssl/rabbit/ca_certificate.pem"},
                    {certfile,"/etc/ssl/rabbit/server_certificate.pem"},
                    {keyfile,"/etc/ssl/rabbit/server_key.pem"},
                    {verify,verify_peer},
                    {fail_if_no_peer_cert,true}]}
   ]}
].